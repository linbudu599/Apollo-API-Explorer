{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/node" },
  "defaultProject": "tmp",
  "projects": {
    "tmp": {
      "root": "apps/tmp",
      "sourceRoot": "apps\\tmp\\src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/tmp",
            "main": "apps/tmp/src/main.ts",
            "tsConfig": "apps/tmp/tsconfig.app.json",
            "assets": ["apps/tmp/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/tmp/src/environments/environment.ts",
                  "with": "apps/tmp/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "tmp:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/tmp/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/tmp"],
          "options": {
            "jestConfig": "apps/tmp/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "app1": {
      "root": "apps/app1",
      "sourceRoot": "apps\\app1\\src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/app1",
            "main": "apps/app1/src/main.ts",
            "tsConfig": "apps/app1/tsconfig.app.json",
            "assets": ["apps/app1/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/app1/src/environments/environment.ts",
                  "with": "apps/app1/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "app1:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/app1/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app1"],
          "options": {
            "jestConfig": "apps/app1/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
