{
  "name": "GraphQL-Explorer-Server",
  "version": "1.0.0",
  "description": "API Usage of GraphQL Stack",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only ./server/app.ts",
    "prebuild": "npm run genql && node ./scripts/clean.js --server && npm run prisma",
    "build": "cross-env NODE_ENV=production tsc",
    "postbuild": "node ./scripts/prisma/copy.js && npm run prisma:db:prod && node ./dist/prisma/seed.js",
    "build:vercel": "npm run genql && cross-env NODE_ENV=production node ./scripts/clean.js --vercel && tsc --build vercel-tsconfig.json",
    "clean": "node ./scripts/clean.js --server --vercel",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production nodemon ./dist/app.js",
    "start:dev": "rimraf db-prod.sqlite && cross-env NODE_ENV=production nodemon ./dist/app.js",
    "pm2": "npm run build && pm2 start ecosystem.config.js",
    "stress-test": "node ./scripts/st-main.js --time 1000 * 60",
    "voyager": "serve -s voyager/local.html",
    "gen:code": "graphql-codegen --config codegen.yml",
    "gen:docs": "rimraf -rf ./docs && graphdoc --force",
    "gen": " node ./scripts/gen/main.js --docs --code",
    "serve:docs": "npm run gen:docs && serve -s docs",
    "commit": "git add -A && git-cz",
    "typecheck": "tsc --noEmit",
    "prod:vercel": "vc -prod",
    "prettier": "prettier --write ./**/*.ts ./**/*.js ./**/*.json ./**/*.graphql",
    "seed:config": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "genql": "genql --schema ./server/typegraphql/schema.graphql --output ./server/genql-generated",
    "prisma": "npm run prisma:db && npm run prisma:generate",
    "prisma:db": "prisma db push --preview-feature",
    "prisma:db:prod": "prisma db push --schema=./dist/prisma/schema.prisma --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:seed": "ts-node ./server/prisma/seed.ts",
    "generate": "ts-node ./scripts/command/index.ts generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linbudu599/GraphQL-Explorer-Server.git"
  },
  "keywords": [
    "GraphQL",
    "TypeGraphQL",
    "Apollo-Server"
  ],
  "graphdoc": {
    "endpoint": "http://localhost:4000/graphql",
    "output": "./docs"
  },
  "prisma": {
    "schema": "server/prisma/schema.prisma"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "author": "linbudu599",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linbudu599/GraphQL-Explorer-Server/issues"
  },
  "homepage": "https://github.com/linbudu599/GraphQL-Explorer-Server#readme",
  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@genql/runtime": "^2.6.0",
    "@prisma/client": "^2.29.1",
    "@saeris/apollo-server-vercel": "^0.4.0",
    "apollo-datasource-rest": "^3.1.1",
    "apollo-server": "^3.1.2",
    "apollo-server-core": "^3.1.2",
    "apollo-server-koa": "^3.1.2",
    "apollo-server-plugin-base": "^3.1.1",
    "apollo-server-plugin-response-cache": "^3.1.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "dataloader": "^2.0.0",
    "dateformat": "^4.5.1",
    "graphql": "^15.5.1",
    "graphql-query-complexity": "^0.9.0",
    "graphql-tag": "^2.12.5",
    "graphql-tools": "^8.1.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "graphqldoc": "^2.5.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.1",
    "numeral": "^2.0.6",
    "sqlite3": "^5.0.2",
    "type-graphql": "^1.1.1",
    "type-graphql-dataloader": "^0.4.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.37",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "^0.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@genql/cli": "^2.6.0",
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/fragment-matcher": "^3.1.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/time": "^3.1.0",
    "@graphql-codegen/typescript": "^2.1.0",
    "@graphql-codegen/typescript-operations": "^2.1.0",
    "@graphql-codegen/typescript-resolvers": "^2.1.0",
    "@graphql-codegen/typescript-type-graphql": "^2.1.0",
    "@ice/spec": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/dateformat": "^3.0.1",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.7.1",
    "@types/numeral": "^2.0.1",
    "chalk": "^4.1.2",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.6",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "pm2": "^5.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "prisma": "^2.29.1",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "superagent": "^6.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
